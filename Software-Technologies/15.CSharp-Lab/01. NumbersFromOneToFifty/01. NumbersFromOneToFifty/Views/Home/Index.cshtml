@{
    ViewBag.Title = "Home Page";
}

<div class="row">
    <div class="col-md-4">
        <h2>Numbers:</h2>
        <ul class="list-group">
            @for (int i = 1; i <= 50; i++)
            {
                <li class="list-group-item">@i</li>
            }
        </ul>
    </div>
    <div class="col-md-4">
        <h2>Alphabet:</h2>
        <ul class="list-group">
            @for (char i = 'A'; i <= 'Z'; i++)
            {
                <li class="list-group-item">@i</li>
            }
        </ul>
    </div>
    <div class="col-md-4">
        <h2>Азбука:</h2>
        <ul class="list-group">
            @for (char i = 'А'; i <= 'Я'; i++)
            {
                <li class="list-group-item">@i</li>
            }
        </ul>
    </div>
</div>